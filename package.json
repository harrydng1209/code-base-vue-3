{
  "name": "code-base-vue-3",
  "description": "A modular and maintainable Vue 3 web application codebase, featuring reusable components, scalable folder structure, and state management using best practices for modern web development",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "main": "src/main.ts",
  "author": {
    "name": "HXRRY",
    "email": "harrydng1209@gmail.com",
    "url": "https://github.com/harrydng1209"
  },
  "devEngines": {
    "node": ">=20.x",
    "npm": ">=10.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,json}\"",
    "preinstall": "npx -y only-allow pnpm",
    "prepare": "husky",
    "precommit": "lint-staged && validate-branch-name"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "pnpm run lint",
      "pnpm run format",
      "git add ."
    ]
  },
  "validate-branch-name": {
    "pattern": "^(feature|bugfix|hotfix|release)/.+|(master)$"
  },
  "dependencies": {
    "@vee-validate/yup": "^4.13.2",
    "@vueuse/core": "^11.1.0",
    "@vueuse/shared": "^11.1.0",
    "axios": "^1.7.7",
    "dotenv": "^16.4.7",
    "element-plus": "^2.8.5",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "qs": "^6.13.0",
    "string-template": "^1.0.0",
    "tippy.js": "^6.3.7",
    "uuid": "^10.0.0",
    "vee-validate": "^4.13.2",
    "vue": "^3.4.29",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.3.3",
    "vue-tippy": "^6.4.4",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.5",
    "@types/qs": "^6.9.16",
    "@types/string-template": "^1.0.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.12.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.1.6",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.47",
    "prettier": "^3.2.5",
    "sass": "1.77.6",
    "start-server-and-test": "^2.0.4",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "validate-branch-name": "^1.3.1",
    "vite": "^5.3.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  }
}
